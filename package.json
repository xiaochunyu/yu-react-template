    "stylelint-scss": "^5.3.1",
    "stylus": "^0.61.0",
    "stylus-loader": "^7.1.3",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
